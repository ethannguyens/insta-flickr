{
  "name": "insta-flickr",
  "version": "1.0.0",
  "description": "Insta Flickr",
  "main": "app/tools/build.js",
  "scripts": {
    "prestart": "babel-node app/tools/startMessage.js",
    "start": "npm-run-all --parallel test:browser open:src lint:watch",
    "open:src": "export NODE_ENV='dev' && babel-node app/tools/srcServer.js",
    "open:test": "node app/test/open.js",
    "lint": "node_modules/.bin/esw webpack.config.* app src",
    "lint:watch": "npm run lint -- --watch",
    "test": "npm run test:browser",
    "test:node": "mocha --require ignore-styles --reporter progress app/test/nodeTestSetup.js \"src/**/*.test.js\" \"app/**/*.test.js\" ",
    "test:browser": "npm run remove:test && npm-run-all --parallel open:test unit:chrome",
    "unit:chrome": "webpack-dev-server --config webpack.test.config.js --content-base --port 3003 --hot --inline",
    "test-single": "mocha --reporter progress app/tools/testSetup.js",
    "test-single:watch": "npm run test-single -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node app/tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test:browser lint build:html",
    "build": "babel-node app/tools/build.js",
    "postbuild": "babel-node app/tools/distServer.js",
    "babel-test": "babel-node src/modules/word-prime-count",
    "heroku-prebuild": "yarn install && npm-run-all clean-dist lint build:html",
    "heroku-postbuild": "babel-node app/tools/build.js",
    "remove:test": "rm -rf app/test/bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ethannguyens/ctm.git"
  },
  "keywords": [
    "ctm"
  ],
  "author": "Ethan Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ethannguyens/ctm/issues"
  },
  "homepage": "https://github.com/ethannguyens/ctm#readme",
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "compression": "1.6.1",
    "express": "4.13.4",
    "fetch-jsonp": "^1.1.3",
    "path": "0.12.7",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "redux": "3.5.2",
    "redux-form": "7.0.3",
    "redux-thunk": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.8.0",
    "cheerio": "1.0.0-rc.2",
    "css-loader": "0.23.1",
    "enzyme": "2.9.1",
    "enzyme-adapter-react-15": "^1.0.2",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "extract-text-webpack-plugin": "2.1.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "8.1.0",
    "json-loader": "^0.5.7",
    "mocha": "3.5.0",
    "mocha-loader": "^1.1.1",
    "nock": "9.0.14",
    "node-sass": "3.13.0",
    "npm-run-all": "1.8.0",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "prop-types": "15.5.10",
    "react-test-renderer": "^16.0.0",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
    "sass-loader": "4.0.2",
    "sauce-connect-launcher": "^1.2.2",
    "sinon": "^4.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "3.4.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "2.10.0",
    "xhr-mock": "^1.9.1"
  }
}
